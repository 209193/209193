#include <fstream>
#include <iostream>
#include <cstdlib>

#define SIZE 10000000

int main() {

  int tab[SIZE];

  for(int i=0; i<SIZE; ++i)
    tab[i] = (std::rand() % 50)+1;

  std::ofstream data("data.txt");
  if (data.is_open()){
    for (int i=0; i<SIZE; ++i)
      data << tab[i] << ' ';
    data.close();
  }
  else std::cout << "Unable to open file" << std::endl;

  return 0;
}
